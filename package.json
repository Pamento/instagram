{
  "name": "instagram",
  "version": "1.0.0",
  "description": "simply application web",
  "main": "index.js",
  "scripts": {
    "test": "echo 'Error: no test specified' && exit 1",
    "start": "nodemon indes.js",
    "mysql-start": "sudo docker-compose up",
    "mysql-stop": "sudo docker-compose down",
